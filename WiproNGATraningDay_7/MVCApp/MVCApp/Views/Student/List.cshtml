@{
}
@model List<MVCApp.Models.Student>

<h2>Student List</h2>

<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>College</th>
        <th>Department</th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Id</td>
            <td>@student.Name</td>
            <td>@student.College</td>
            <td>@student.Dept</td>
        </tr>
    }
</table>

<a href="/Student/Add">Add New Student</a>

